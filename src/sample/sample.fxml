   </rowConstraints>
   <children>
      <Pane prefHeight="620.0" prefWidth="862.0">
         <children>
            <Button fx:id="btn_read" layoutX="33.0" layoutY="14.0" mnemonicParsing="false" onAction="#btn_click" text="read" />
            <TextArea fx:id="Text_input" layoutY="70.0" prefHeight="550.0" prefWidth="475.0" />
            <TabPane layoutX="502.0" layoutY="7.0" prefHeight="613.0" prefWidth="360.0" tabClosingPolicy="UNAVAILABLE">
